{
	"interface": {
		"dataField": {
			"default": "<__element__></__element__>",
			"title": "<label>$t(__title__)</label>",
			"accordionTitle": "<a href=\"#__link__\">$t(__title__)</h3>",
			"progressBar": "<div class=\"progress\"><div></div></div>",
			"slider":[
				"<div class=\"range-slider\" data-slider data-options=\"__options__\">",
					"<span class=\"range-slider-handle\"></span>",
					"<span class=\"range-slider-active-segment\"></span>",
					"<input type=\"hidden\">",
				"</div>"
			],
			"choiceToggle": {
				"link": "<a href=\"#\" data-options=\"__alignment__\" data-dropdown=\"__link__\" class=\"dataField-button\">__label__</a>",
				"listContainer": "<ul id=\"__link__\" class=\"f-dropdown\" data-dropdown-content></ul>",
				"listItem": "<li></li>"
			}
		},
		"evolution": {
			"tooltipBase": "<div><h3>__name__</h3></div>",
			"tooltipRow": "<div class=\"row\"><div class=\"label\">$t(__label__)</div><div class=\"value\">__value__</div></div>",
			"tooltipDesc": "<p></p>"
		},
		"mutation":{
			"grid":[
				"<div id=\"tb_board\">",
					"<div><ul class=\"grid\"></ul></div>",
				"</div>"
			],
			"piece_container":[
				"<ul id=\"tb_toolbox\">",
				"</ul>"
			],
			"piece":[
				"<li class=\"geneBlock\">",
					"<p class=\"image\"><img src=\"\" draggable=\"false\" /></p>",
					"<p class=\"name\"></p>",
				"</li>"
			]
		}
	},
	"alerts": {
		"win_message": "<p>$t(messages:win_message)</p>",
		"lose_message": "<p>$t(messages:lose_message)</p>"
	}
}